        function format(time) {
            // Hours, minutes and seconds
            let hrs = ~~(time / 3600);
            let mins = ~~((time % 3600) / 60);
            let secs = ~~time % 60;

            // Output like "1:01" or "4:03:59" or "123:03:59"
            let ret = "";
            if (hrs > 0) {
                ret += "" + hrs + ":" + (mins < 10 ? "0" : "");
            }
            ret += "" + mins + ":" + (secs < 10 ? "0" : "");
            ret += "" + secs;
            return ret;
        }
        let remaining = <%=remaining%>;

        // $(document).on('change', '#first', ()=>{
        //     $('#change1q').fadeIn(500).text(questions[$('#first').val()][0]);
        //     $('#change1').slideDown(500).show();
        //
        // })
        // $(document).on('change', '#second', ()=>{
        //     $('#change2q').text(questions[$('#second').val()][0]).fadeIn(500);
        //     $('#change2').slideDown(500).show();
        // });
        let i = 0
        $(".teamchoice").on('change', async function() {
            let val = $(this).val()
            let id = $(this).attr('id')
            let Qdiv, divid
            if (id === "first"){
                divid = "#change1"
                Qdiv = $(`#change1`)
            } else {
                divid = "#change2"
                Qdiv = $(`#change2`)
            }
            Qdiv.slideDown("slow").show()
            if (questions[val])
            {
                console.log(questions[val])
                Qdiv.slideUp("slow", async ()=>{
                    console.log("up")
                    Qdiv.empty()
                    await questions[val].forEach((question)=>
                    {
                        console.log(question)
                        Qdiv.append(`<div class="row">
